{
  "name": "my-app",
  "version": "1.0.0",
  "description": "Sample app to submit email",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "vercel-build": "npm install"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.3",
    "connect-mongo": "^5.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "mongodb": "^6.15.0",
    "mongoose": "^8.13.0",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^5.0.2",
    "nodemailer": "^6.10.0",
    "ws": "^8.18.1"
  },
  "services": {
    "type": "web",
    "name": "my-app",
    "env": "node",
    "plan": "free",
    "buildCommand": "npm install",
    "startCommand": "npm start",
    "envVars": [
      {
        "key": "NODE_ENV",
        "value": "production"
      }
    ]
  }
}
