{
  "name": "my-app",
  "version": "1.0.0",
  "description": "Sample app to submit email",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "vercel-build": "npm install"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.3",
    "cloudinary": "^1.21.0",
    "connect-mongo": "^5.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.15.0",
    "mongoose": "^8.13.0",
    "multer": "^1.4.5-lts.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.10.0",
    "ws": "^8.18.1"
  },
  "services": {
    "type": "web",
    "name": "my-app",
    "env": "node",
    "plan": "free",
    "buildCommand": "npm install",
    "startCommand": "npm start",
    "envVars": [
      {
        "key": "NODE_ENV",
        "value": "production"
      }
    ]
  },
  "devDependencies": {
    "tailwindcss": "^4.1.4"
  }
}
